<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragmentos.RepuestosAutorizadasFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/panel_encabezado" />

        <Button
            android:id="@+id/btn_siguiente_repuestos_autorizadas"
            style="@style/letra_contenido_blanco"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:background="@drawable/redondo_botones"
            android:text="Siguiente"
            android:textAllCaps="false" />

    </LinearLayout>

</FrameLayout>